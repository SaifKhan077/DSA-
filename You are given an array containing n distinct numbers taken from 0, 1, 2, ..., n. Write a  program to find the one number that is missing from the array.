public class ltask4 {
    public static void main(String[] args) {
        int arr[] = {0,1,3,4};
        System.out.println("Number is missing :: " + m(arr));
    }
    static int m(int [] arr){
        int num = 0;
        for(int i = 0 ; i <= arr.length -1 ; i ++){
            if (arr[i] != i){
                num = i;
                break;
            }
        }
        return num;
    }
}

