public class task1 {

    public static void main(String[] args) {

        int a[] = {11, 12, 13, 14};
        int b[] = {21, 22, 23, 24};

        for (int i = 0; i < a.length; i++) {
            int temp = a[i];
            a[i] = b[i];
            b[i] = temp;
        }
        for (int first : a) {
            System.out.println("First Array a[] Elements:: " + first);
        }

        System.out.println();

        for (int second : b) {
            System.out.println("Second Array b[] Elements:: " + second);
        }
    }
}


